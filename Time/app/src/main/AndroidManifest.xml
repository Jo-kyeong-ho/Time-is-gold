        <activity android:name=".SettingValue" />
        <activity android:name=".SelectStart" />
        <activity android:name=".SelectSetting" />
        <activity android:name=".DBShow" />
        <activity android:name=".GetPicture" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

